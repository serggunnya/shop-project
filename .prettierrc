{
	"semi": true,
	"trailingComma": "all",
	"printWidth": 100,
	"useTabs": true,
	"endOfLine": "lf",
	"arrowParens": "always",
	"importOrder": ["^react$", "<THIRD_PARTY_MODULES>", "^@/", "^[.]"],
	"importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
