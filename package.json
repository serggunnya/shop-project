{
  "name": "shoppirest",
  "version": "0.0.1",
  "description": "интернет магазин",
  "author": "github.com/serggunnya",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "",
    "prisma:gen": "npx prisma generate",
    "prisma:push": "npx prisma db push",
    "prisma:seed": "npx prisma db seed",
    "nest:dev": "rimraf server && nest start --watch",
    "react:dev": "cd react && rimraf build && npm start",
    "start:dev": "concurrently \"npm run nest:dev\" \"npm run react:dev\"",
    "nest:build": "rimraf server && nest build"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^6.0.5",
    "@prisma/client": "^3.15.2",
    "@supabase/supabase-js": "^2.4.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "argon2": "^0.28.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "concurrently": "^8.2.1",
    "nestjs-supabase-auth": "^1.0.9",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "prisma-class-generator": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/jest": "27.5.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "prisma": "^3.15.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3"
  },
  "prisma": {
    "seed": "ts-node prisma\\seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
